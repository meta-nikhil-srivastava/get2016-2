<!-- Use this file to deploy some handlers/chains and services      -->
<!-- Two ways to do this:                                           -->
<!--   java org.apache.axis.client.AdminClient deploy.wsdd          -->
<!--      after the axis server is running                          -->
<!-- or                                                             -->
<!--   java org.apache.axis.utils.Admin client|server deploy.wsdd   -->
<!--      from the same directory that the Axis engine runs         -->

<deployment
    xmlns="http://xml.apache.org/axis/wsdd/"
    xmlns:java="http://xml.apache.org/axis/wsdd/providers/java">

  <!-- Services from TemperatureConversionService WSDL service -->

  <service name="TemperatureConversion" provider="java:RPC" style="wrapped" use="literal">
      <parameter name="wsdlTargetNamespace" value="http://web"/>
      <parameter name="wsdlServiceElement" value="TemperatureConversionService"/>
      <parameter name="schemaQualified" value="http://web"/>
      <parameter name="wsdlServicePort" value="TemperatureConversion"/>
      <parameter name="className" value="web.TemperatureConversionSoapBindingImpl"/>
      <parameter name="wsdlPortType" value="TemperatureConversion"/>
      <parameter name="typeMappingVersion" value="1.2"/>
      <operation name="fahrenheitToCelsius" qname="operNS:fahrenheitToCelsius" xmlns:operNS="http://web" returnQName="retNS:fahrenheitToCelsiusReturn" xmlns:retNS="http://web" returnType="rtns:double" xmlns:rtns="http://www.w3.org/2001/XMLSchema" soapAction="" >
        <parameter qname="pns:fahrenheit" xmlns:pns="http://web" type="tns:double" xmlns:tns="http://www.w3.org/2001/XMLSchema"/>
      </operation>
      <parameter name="allowedMethods" value="fahrenheitToCelsius"/>

  </service>
</deployment>
